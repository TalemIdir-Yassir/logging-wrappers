<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="CLOUD" class="com.google.cloud.logging.logback.LoggingAppender">
        <loggingEventEnhancer>com.yassir.logger.CustomEnhancer</loggingEventEnhancer>
    </appender>

    <root level="info">
        <appender-ref ref="CLOUD" />
    </root>

</configuration>